{
  "manifest_version": 3,
  "name": "Tao Wallet",
  "version": "0.0.1",
  "description": "Bittensor wallet extension.",
  "side_panel": {
    "default_path": "index.html"
  },
  "action": {
    "default_title": "Open Tao Wallet"
  },
  "permissions": [
    "sidePanel"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
